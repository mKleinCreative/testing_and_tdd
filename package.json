{
  "name": "testing_and_tdd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "test": "ava --tap | tap-nyan",
    "test:diff": "ava --tap | tap-summary",
    "test:report": "nyc ava",
    "db:latest": "knex --knexfile database/knexfile.js migrate:latest",
    "db:rollback": "knex --knexfile database/knexfile.js migrate:rollback",
    "db:seed": "knex --knexfile database/knexfile.js seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminpettus/testing_and_tdd.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjaminpettus/testing_and_tdd/issues"
  },
  "homepage": "https://github.com/benjaminpettus/testing_and_tdd#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "dark-sky-api": "^0.5.24",
    "express": "4.15.2",
    "forecastio": "^1.0.2",
    "knex": "0.12.9",
    "material-design-lite": "1.3.0",
    "morgan": "^1.8.1",
    "pg": "6.1.5",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "webpack": "2.4.1"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-regenerator": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.4.0",
    "babel-preset-react": "6.24.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-change": "2.1.2",
    "chai-http": "3.0.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-plugin-ava": "4.2.0",
    "html-webpack-plugin": "2.28.0",
    "mocha": "3.2.0",
    "nock": "^9.0.13",
    "nyc": "10.2.0",
    "pify": "2.3.0",
    "sinon": "2.1.0",
    "tap-diff": "0.1.1",
    "tap-nyan": "1.1.0",
    "tap-spec": "4.1.1",
    "tap-summary": "3.0.2",
    "tape": "4.6.3"
  },
  "ava": {
    "files": [
      "tests/**/**/*.js",
      "!**/controller_tests.js",
      "!**/apiHandlers_test.js",
      "!**/webserver/*"
    ],
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "ava"
    ],
    "extends": "plugin:ava/recommended"
  }
}
